{
  "name": "neufund-contracts",
  "version": "1.0.0",
  "description": "Some basic smart contracts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "testrpc": "testrpc",
    "compile": "truffle compile",
    "test": "truffle test",
    "lint": "solium --dir contracts; eslint ./",
    "build": "webpack",
    "dev": "webpack-dev-server --hot --https --open --colors --progress --history-api-fallback",
    "ipfs": "webpack -p && ipfs add -r build | tail -n1 | cut -d' ' -f 2 | xargs ipfs name publish",
    "serve": "truffle serve",
    "watch": "truffle watch",
    "start": "webpack-dev-server --host 0.0.0.0 --port 3000 --hot --content-base build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Neufund/Contracts.git"
  },
  "author": "Remco Bloemen <remco@neufund.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Neufund/Contracts/issues"
  },
  "homepage": "https://github.com/Neufund/Contracts#readme",
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "ethereumjs-testrpc": "^3.0.3",
    "ledger-wallet-provider": "^1.6.5",
    "material-ui": "^0.17.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^4.0.0",
    "react-select": "^1.0.0-rc.3",
    "react-tap-event-plugin": "^2.0.1",
    "truffle": "^3.0.4",
    "truffle-contract": "^1.1.9",
    "web3-polyfill": "^1.1.0",
    "zeppelin-solidity": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-register": "^6.22.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.27.3",
    "eslint": "^3.14.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "ethereumjs-testrpc": "^3.0.3",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.4",
    "react-hot-loader": "^3.0.0-beta",
    "solc": "^0.4.10",
    "solium": "^0.2.2",
    "style-loader": "^0.14.1",
    "truffle-contract": "^1.1.6",
    "truffle-solidity-loader": "0.0.8",
    "url-loader": "^0.5.7",
    "web3": "^0.18.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
}
